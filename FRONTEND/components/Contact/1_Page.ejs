<form action="http://localhost:3001/api" method="post">
    <div>
        <input type="FirstName" id="FirstName" name="FirstName" placeholder="FirstName *" required>
    </div>
    <div>
        <input type="LastName" id="LastName" name="LastName" placeholder="LastName *" required>
    </div>
    <div>
        <input type="email" id="email" name="email" placeholder="Email *">
    </div>
    <div>
        <input type="date" id="date" name="date" required>
    </div>
    <div>
        <textarea name="msg" id="msg" placeholder="Hello my gorgeous friend in the internet" cols="30"
            rows="10"></textarea>
    </div>

    <button type="submit"> Submit </button>
</form>